<script type="text/javascript">

var script = document.createElement('script');
script.src = "//www.googletagmanager.com/gtm.js?id=GTM-NDV9X9";
document.getElementsByTagName("head")[0].appendChild(script);

window.criteo_q = window.criteo_q || [];

var productBasketProducts = [];

var itemList = document.querySelectorAll('.cart-deal-item');

for(var idx = 0; idx < itemList.length; idx++) {
	var cartItem = itemList[idx],
		cartItemAttr = cartItem.attributes;

	for(itemAttrIdx = 0; itemAttrIdx < cartItemAttr.length; itemAttrIdx++) {
		if(cartItemAttr[itemAttrIdx].name == 'data-item-status' && cartItemAttr[itemAttrIdx].value == 'CHECKED') {

    	var productAttrList = cartItem.querySelectorAll('.option-item')[0].attributes,
			productPriceAttrList = cartItem.querySelectorAll('.option-price-part')[0].attributes,
			productId = '',
			vendorItemPackageId = '',
			price = 0,
			quantity = 0;

      for (var attrIdx = 0; attrIdx < productAttrList.length; attrIdx++) {
        if (productAttrList[attrIdx].name == 'data-pid') {
          productId = productAttrList[attrIdx].value;
        }
        if (productAttrList[attrIdx].name == 'data-vip') {
          if (productAttrList[attrIdx].value && parseInt(productAttrList[attrIdx].value) > 0) {
            vendorItemPackageId = productAttrList[attrIdx].value;
          }
        }
        if (productAttrList[attrIdx].name == 'data-quantity') {
          quantity = productAttrList[attrIdx].value;
        }
      }

      for (var attrIdx = 0; attrIdx < productPriceAttrList.length; attrIdx++) {
        if (productPriceAttrList[attrIdx].name == 'data-sale-price') {
          price = productPriceAttrList[attrIdx].value;
        }
      }

      // id에는 vendorItemPackageId가 있는 경우에는 vendorItemPackageId, 없으면 productId를 넣어준다.
      // vendorItemPackageId에 -1이 들어가는 경우도 있기 때문에, 0보다 큰 경우로 걸러낸다.
      productBasketProducts.push({
        'id' : (vendorItemPackageId && vendorItemPackageId > 0) ? vendorItemPackageId : productId,
        'price' : price,
        'quantity' : quantity
      })

    }
	}
}

if(productBasketProducts.length > 0) {
  window.criteo_q.push(
    { event: "setAccount", account: 3119 },
    { event: "setSiteType", type: "d" },
    { event: "viewBasket", item: productBasketProducts});
}

</script>

